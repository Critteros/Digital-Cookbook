{
  "name": "javaflavors-front",
  "version": "0.0.1",
  "packageManager": "yarn@4.1.1",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "db:run": "tsx --tsconfig ./.svelte-kit/tsconfig.json node_modules/.bin/drizzle-kit",
    "db:generate": "yarn db:run generate:pg",
    "db:migrate": "tsx src/lib/server/db/migrate.ts"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-node": "^5.1.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@types/eslint": "^8.56.0",
    "@types/memoizee": "^0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "autoprefixer": "^10.4.16",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.20.17",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "houdini": "^1.2.47",
    "houdini-svelte": "^1.2.47",
    "postcss": "^8.4.32",
    "postcss-load-config": "^5.0.2",
    "prettier": "3.2.5",
    "prettier-plugin-svelte": "^3.1.2",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.3.6",
    "tslib": "^2.4.1",
    "tsx": "^4.8.2",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "vitest": "^1.2.0"
  },
  "dependencies": {
    "@iconify/svelte": "^4.0.2",
    "@lucia-auth/adapter-drizzle": "^1.0.7",
    "bits-ui": "^0.21.10",
    "clsx": "^2.1.0",
    "drizzle-orm": "^0.30.10",
    "flowbite-svelte-icons": "^1.6.1",
    "formsnap": "^1.0.0",
    "http-status-codes": "^2.3.0",
    "jwt-decode": "^4.0.0",
    "lucia": "^3.2.0",
    "lucide-svelte": "^0.379.0",
    "memoizee": "^0.4.17",
    "mode-watcher": "^0.3.0",
    "oslo": "^1.2.0",
    "pino": "^9.0.0",
    "pino-pretty": "^11.0.0",
    "postgres": "^3.4.4",
    "svelte-radix": "^1.1.0",
    "sveltekit-i18n": "^2.4.2",
    "sveltekit-superforms": "^2.14.0",
    "tailwind-merge": "^2.2.2",
    "tailwind-variants": "^0.2.1",
    "zod": "^3.23.8"
  }
}
